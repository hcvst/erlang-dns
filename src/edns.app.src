{application, edns,
  [{description, "A Erlang OTP DNS Server"},
   {vsn, "0.1-alpha"},
   {modules, []},
   {registered, [ed_sup]},
   {applications, [kernel, stdlib]},
   {mod, {ed_app, []}},
   {env, [
     {udp_port, 1053},
     {tcp_port, 1053},
     {resolvers, [ed_query_resolver, simple_stats_resolver]},
     {master_mode, false},
     {slave_mode, false},
     {supermasters, []}
   ]}
  ]
}.
